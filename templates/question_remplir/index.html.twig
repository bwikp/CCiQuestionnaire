                <td>{{ question_remplir.reponse2 }}</td>
                <td>{{ question_remplir.detail3 }}</td>
                <td>{{ question_remplir.reponse3 }}</td>
                <td>{{ question_remplir.detail4 }}</td>
                <td>{{ question_remplir.reponse4 }}</td>
                <td>{{ question_remplir.note }}</td>
                <td>
                    <a href="{{ path('app_question_remplir_show', {'id': question_remplir.id}) }}">show</a>
                    <a href="{{ path('app_question_remplir_edit', {'id': question_remplir.id}) }}">edit</a>
