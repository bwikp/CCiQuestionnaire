{% extends 'base.html.twig' %}

{% block title %}QuestionRemplir index{% endblock %}

{% block body %}
    <h1>QuestionRemplir index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Detail</th>
                <th>Detail_1</th>
                <th>Reponse1</th>
                <th>Detail2</th>
                <th>Reponse2</th>
                <th>Detail3</th>
                <th>Reponse3</th>
                <th>Detail4</th>
                <th>Reponse4</th>
                <th>Note</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for question_remplir in question_remplirs %}
