{% extends 'base.html.twig' %}

{% block title %}Resultat index{% endblock %}

{% block body %}
    <h1>Resultat index</h1>
	<div class="container">

    <table class="table table-bordered">
        <thead>
            <tr>
                <th>Id</th>
                <th>Score_final</th>
                <th>Is_admis</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for resultat in resultats %}
            <tr>
                <td>{{ resultat.id }}</td>
                <td>{{ resultat.scoreFinal }}</td>
                <td>{{ resultat.isAdmis ? 'Yes' : 'No' }}</td>
                <td>
                    <a href="{{ path('app_resultat_show', {'id': resultat.id}) }}">show</a>
                    <a href="{{ path('app_resultat_edit', {'id': resultat.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
</div>
    <a href="{{ path('app_resultat_new') }}">Create new</a>
{% endblock %}
